{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "path": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^\\/([a-z0-9-_]+)?$",
      "required": true,
      "unique": false,
      "default": "/"
    },
    "year": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": "^\\d{4}$",
      "default": "2022",
      "minLength": 4,
      "maxLength": 4,
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "primaryColor": {
      "type": "string",
      "required": true
    },
    "secondaryColor": {
      "type": "string",
      "required": true
    },
    "accentColor": {
      "type": "string",
      "required": true
    },
    "section": {
      "displayName": "section",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "section.section"
    },
    "nollekamp": {
      "displayName": "nollekamp",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "nollekamp.nollekamp"
    },
    "phos": {
      "displayName": "phoset",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "phoset.phoset"
    }
  }
}
