{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "path": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^\\/([a-z0-9-_]+)?$",
      "required": true,
      "unique": false,
      "default": "/"
    },
    "year": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "default": 2022,
      "required": true
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "section": {
      "displayName": "section",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "section.section"
    },
    "nollekamp": {
      "displayName": "nollekamp",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "nollekamp.nollekamp"
    }
  }
}
