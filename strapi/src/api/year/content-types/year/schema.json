{
  "kind": "collectionType",
  "collectionName": "years",
  "info": {
    "singularName": "year",
    "pluralName": "years",
    "displayName": "Years",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "year": {
      "type": "string",
      "regex": "^\\d{4}$",
      "required": true,
      "unique": false,
      "maxLength": 4,
      "minLength": 4,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "primaryColor": {
      "type": "string",
      "default": "#FFFFFF",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "secondaryColor": {
      "type": "string",
      "default": "#FFFFFF",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "accentColor": {
      "type": "string",
      "default": "#FFFFFF",
      "regex": "^#(?:[0-9A-F]{3}){1,2}$",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "password": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content.images",
        "content.nollekamp",
        "content.phoset",
        "content.sponsors",
        "content.text",
        "content.calendar"
      ]
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    }
  }
}
